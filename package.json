  },
  "dependencies": {
    "@types/chai": "^3.4.34",
    "@types/csv-parse": "^1.1.6",
    "@types/mocha": "^2.2.36",
    "@types/mongoose": "^4.7.5",
    "@types/node-fetch": "^1.6.7",
    "@types/restify": "^2.0.39",
    "@types/restify-plugins": "^1.5.4",
    "@types/sinon": "^1.16.35",
    "@types/supertest": "^2.0.0",
    "csv-parse": "^1.1.7",
    "dotenv": "^4.0.0",
    "mongoose": "^4.8.2",
    "mongoose-findorcreate": "^1.0.1",
    "node-fetch": "^1.6.3",
    "passport-github": "^1.1.0",
    "passport-restify": "^1.0.1",
    "restify": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "supertest": "^3.0.0",
    "tslint": "^4.3.1",
    "tslint-eslint-rules": "^3.2.3",
    "typescript": "^2.2.0"
  },
  "scripts": {
    "clean": "node tsc-cleanup",
    "lint": "node_modules/.bin/tslint --project .",
    "compile": "node_modules/.bin/tsc",
    "build": "npm run clean && npm run lint && npm run compile",
    "test": "npm run build && NODE_ENV='test' node_modules/.bin/_mocha \"test/**/*.spec.js\"",
    "test:debug": "DEBUG=true npm run test",
    "cover": "npm run build && NODE_ENV='test' node_modules/.bin/istanbul cover node_modules/.bin/_mocha \"test/**/*.spec.js\"",
    "start": "npm run build && DEBUG=true node ./build/server.js",
    "start:prod": "npm run build && NODE_ENV='production' node server.js"
  }
}
